INSTALL_DIR=/opt/akana/sandbox/2020.1
PRODUCT_HOME=/opt/akana/sandbox/2020.1
CONTAINER_NAME=gateway
CONTAINER_KEY=10003
CONTAINER_ADMIN=administrator
CONTAINER_ADMIN_PASSWORD=password
DEFAULT_HOST=gateway.sandbox.org
DEFAULT_PORT=9915
DEFAULT_BIND_ALL=true
ROOT_CONTEXT=
SESSION_TIMEOUT=3600

# db-driver.json
DB_DRIVER_LOCATION=file:///opt/akana/resources/db/ojdbc8.jar

# container-identity.json
CONTAINER_IDENTITY_ACTION=generate.pki.keys.and.x509

# security.json
#KEYSTOREALIAS=
#KEYSTORELOCATION=
#KEYSTOREPASSWORD=
TRUSTSTORELOCATION=file:///opt/akana/sandbox/automation_2020.1/trust.jks
TRUSTSTOREPASSWORD=password
CRLENABLED=false

# admin-console.json
ADMIN_CONSOLE_LOCALHOST_ONLY=false
ADMIN_CONSOLE_ACCESS_RESTRICTED=true
ADMIN_CONSOLE_DOMAIN_ENABLED=true
ADMIN_CONSOLE_BASICAUTH_ENABLED=true

# MongoDB
MONGO_USERNAME=
MONGO_PASSWORD=
# MONGO_URL=mongodb://mongo.sandbox.org:27017/Akana?connectTimeoutMS=15000&socketTimeoutMS=1500000&minPoolSize=0&maxPoolSize=100&waitQueueMultiple=5&waitQueueTimeoutMS=10000
MONGOASYNCTHREAD=20

# Policy Manager Connection
PM_SCHEME=https
PM_HOST=platform_vip.sandbox.org
PM_PORT=9900
PM_ADMIN=administrator
PM_PASSWORD=password

# remote-writer-nd.json
LOCALWRITERENABLED=false
REMOTEWRITERENABLED=true
RETRYONUNRECOVERABLEERROR=true

# platform-jetty.json
JETTY_SERVLET_ENABLE=false
NCSA_ACCESS_LOG_ENABLE=false
NCSA_ACCESS_LOG_RETAIN=10
